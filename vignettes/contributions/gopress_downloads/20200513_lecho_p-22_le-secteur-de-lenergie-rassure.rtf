\par \pard \ql \cbpat3 {\fs18 \f1 \cf2 \chcbpat3 L'Echo* - 24 Mar. 2020 -  22}
\par \pard \ql \cbpat3 {\fs12 \f1 \cf2 \chcbpat3 * }
