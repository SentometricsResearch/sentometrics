\pard \ql \cbpat3 {\fs26 \f1 \cf2 \chcbpat3 Les craintes sur le coronavirus reprennent le dessus}
\par \pard \ql \cbpat3 {\fs18 \f1 \cf2 \chcbpat3 L'Echo* - 07 Mar. 2020 -  40}
\par \pard \ql \cbpat3 {\fs12 \f1 \cf2 \chcbpat3 * }
